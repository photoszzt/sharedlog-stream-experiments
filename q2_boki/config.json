{
    "machines": {
        "faas-gateway": { "type": "c5d.large", "role": "manager" },
        "faas-sequencer-1": { "type": "c5d.large", "role": "worker", "mount_instance_storage": "nvme1n1", "labels": ["sequencer_node=true"] },
        "faas-sequencer-2": { "type": "c5d.large", "role": "worker", "mount_instance_storage": "nvme1n1", "labels": ["sequencer_node=true"] },
        "faas-sequencer-3": { "type": "c5d.large", "role": "worker", "mount_instance_storage": "nvme1n1", "labels": ["sequencer_node=true"] },
        "faas-engine-1": { "type": "c5d.large", "role": "worker", "labels": [ "engine_node=true" ] },
        "faas-engine-2": { "type": "c5d.large", "role": "worker", "labels": [ "engine_node=true" ] },
        "faas-engine-3": { "type": "c5d.large", "role": "worker", "labels": [ "engine_node=true" ] },
        "faas-engine-4": { "type": "c5d.large", "role": "worker", "labels": [ "engine_node=true" ] },
        "faas-storage-1": { "type": "c5d.large", "role": "worker", "mount_instance_storage": "nvme1n1", "labels": ["storage_node=true"] },
        "faas-storage-2": { "type": "c5d.large", "role": "worker", "mount_instance_storage": "nvme1n1", "labels": ["storage_node=true"] },
        "faas-storage-3": { "type": "c5d.large", "role": "worker", "mount_instance_storage": "nvme1n1", "labels": ["storage_node=true"] },
        "faas-storage-4": { "type": "c5d.large", "role": "worker", "mount_instance_storage": "nvme1n1", "labels": ["storage_node=true"] },
        "faas-client": { "type": "c5d.large", "role": "client" }
    },
    "services": {
        "zookeeper": { "placement": "faas-gateway" },
        "zookeeper-setup": { "placement": "faas-gateway" },
        "faas-controller": { "placement": "faas-gateway" },
        "faas-gateway": { "placement": "faas-gateway" },
        "faas-storage": { "placement_label": "storage_node", "replicas": 4 },
        "faas-sequencer": { "placement_label": "sequencer_node", "replicas": 3 },
        "faas-engine": { "placement_label": "engine_node", "replicas": 4 },
        "nexmark-source": { "placement_label": "engine_node", "replicas": 4 },
        "q2": { "placement_label": "engine_node", "replicas": 4 },
        "scale": { "placement_label": "engine_node", "replicas": 1 }
    }
}
