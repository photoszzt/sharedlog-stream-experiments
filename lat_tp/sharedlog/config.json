{
    "machines": {
        "faas-gateway": { "type": "c5d.2xlarge", "role": "manager" },
        "faas-engine-1": { "type": "c5d.2xlarge", "role": "worker", "labels": [ "engine_node=true" ] },
        "faas-engine-2": { "type": "c5d.2xlarge", "role": "worker", "labels": [ "engine_node=true" ] },
        "faas-storage-1": { "type": "c5d.2xlarge", "role": "worker", "mount_instance_storage": "nvme1n1", "labels": ["storage_node=true", "sequencer_node=true"] },
        "faas-storage-2": { "type": "c5d.2xlarge", "role": "worker", "mount_instance_storage": "nvme1n1", "labels": ["storage_node=true", "sequencer_node=true"] },
        "faas-storage-3": { "type": "c5d.2xlarge", "role": "worker", "mount_instance_storage": "nvme1n1", "labels": ["storage_node=true", "sequencer_node=true"] },
        "faas-storage-4": { "type": "c5d.2xlarge", "role": "worker", "mount_instance_storage": "nvme1n1", "labels": ["storage_node=true"] },
        "faas-client": { "type": "c5d.xlarge", "role": "client" }
    },
    "services": {
        "zookeeper": { "placement": "faas-gateway" },
        "zookeeper-setup": { "placement": "faas-gateway" },
        "faas-controller": { "placement": "faas-gateway" },
        "faas-gateway": { "placement": "faas-gateway" },
        "faas-storage": { "placement_label": "storage_node", "replicas": 4 },
        "faas-sequencer": { "placement_label": "sequencer_node", "replicas": 3 },
        "faas-engine": { "placement_label": "engine_node", "replicas": 2 },
        "produce": { "placement_label": "engine_node", "replicas": 2 },
        "consume": { "placement_label": "engine_node", "replicas": 2 },
        "scale": { "placement_label": "engine_node", "replicas": 2 }
    }
}
